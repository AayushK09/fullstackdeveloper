nicart {padding-right: 4px;border-radius: 0 4px 4px 0;}b.snap_rupess_text.pay-later-emi-details-btn {padding-left: 4px;border-radius: 4px 0 0 4px;}#sm-widget-btn.snap_emi_txt_minicart.pay-later-emi-details-btn .snap-emi-inst b.dp-class_minicart, b.snap_rupess_text.pay-later-emi-details-btn {background: #AFE137;line-height: 15px;margin-top: -2px;line-height: 16px;}#snap-modalon_page .snap_pay_only_text {color: #0A2236!important;font-weight: 700;}.latest_2_plan_normak .snap_emi_section:nth-child(2) .snap_zero_perct_text {display:none !important;}`;
                        snapOptions.miniCartPosition = 'after';
                        snapOptions.miniCartQuantityParentDivId = '#CartDrawer-CartItems';
                        snapOptions.miniCartVariantParentDivId = '';
                        snapOptions.miniCartMobileDivId = '';

                        clearInterval(id);
                        addChangeEventListener(
                            snapOptions.miniCartQuantityParentDivId,
                            snapOptions.miniCartVariantParentDivId,
                            true
                        );
                        if (snapOptions.isQuickViewCheckout && snapOptions.isQuickViewCheckout !== 'undefined') {
                            addOnClickForCheckingQuickView();
                        }
                        if (snapOptions.removeMiniCartWidget && snapOptions.removeMiniCartWidget !== 'undefined') {
                            removeMiniCartWidgetFn();
                        }
                        if (snapOptions.isTest && snapOptions.isTest !== 'undefined') {
                            addListenerForRemoveMiniCartFn();
                        }
                        addminCartLoadPage();
                        if (snapOptions.delayTime && snapOptions.delayTime !== 'undefined') {
                            let delayTime = 500;
                            delayTime = parseInt(snapOptions.delayTime);
                            let delayId = setTimeout(() => {
                                addChangeEventListener(
                                    snapOptions.miniCartQuantityParentDivId,
                                    snapOptions.miniCartVariantParentDivId,
                                    true
                                );
                                addminCartLoadPage();
                                if (
                                    snapOptions.isQuickViewCheckout &&
                                    snapOptions.isQuickViewCheckout !== 'undefined'
                                ) {
                                    addOnClickForCheckingQuickView();
                                }
                                clearTimeout(delayId);
                            }, delayTime);
                        }
                    }else{
                      if(counter === 2){
                        if(snapOptions.pdpQuantityChange && snapOptions.pdpQuantityChange !== 'undefined'){
                          const quantityTime = setTimeout(() => {
                          clearTimeout(quantityTime);  
                          getQuantity(false);
                          loadOnPage();
                          },snapOptions.pdpQuantityChange);
                        }
                      miniCartQuickViewFn();
                      }
                    }
                    if (counter === 10) {
                        clearInterval(id);
                    }
                }, 1000);
            });
        }
    }
}

const getMiniWidgetHtml = (snapOrderValue) => {
    const priceArray = getPriceArray();
    const isMiniCart = getByIdOrClassName(snapOptions.miniCartDivId);
    
    removeExistingElements('.snap_emi_txt_minicart');

    if (isMiniCart) {
        return getWidgetForPriceRange(snapOrderValue, priceArray, 'miniCartWidget', snapOptions.miniCartWidget2);
    }
};


function addminCartLoadPage() {
    let priceTag = '';
    if (getByIdOrClassName(snapOptions.miniCartPriceSaleDivId)) {
        priceTag = getByIdOrClassName(snapOptions.miniCartPriceSaleDivId);
    } else {
    if(snapOptions.isSubtotalVisible && snapOptions.isSubtotalVisible !== 'undefined'){
        priceTag = snapOptions.isSubtotalVisible;
    }else{
        priceTag = getByIdOrClassName(snapOptions.miniCartPriceDivId);
    }
    }
    if (snapOptions.miniCartOrderValue && snapOptions.miniCartOrderValue !== 'undefined') {
        if (!priceTag) return;
    }
    if (snapOptions.isTest && snapOptions.isTest !== 'undefined') {
        if (!priceTag) return;
    }
    if(snapOptions.isSubtotalVisible && snapOptions.isSubtotalVisible !== 'undefined'){
    snapOrderValue = snapOptions.isSubtotalVisible;
    }else{
    snapOrderValue = parseNum(priceTag.innerText);
    }
    if (!priceTag) {
        throw new Error('EMPTY_ORDER_VALUE');
    }
    if (findPopup() === 0) {
        let element = getByIdOrClassName('.snap_emi_txt_minicart');
        if (element) element.parentNode.removeChild(element);
        if (snapOptions.miniCartErrorWidgetPage && snapOptions.miniCartErrorWidgetPage !== 'undefined')
            addingMiniCartErrorWidget(snapOrderValue);
        return;
    } else {
        if (snapOptions.miniCartErrorWidgetPage && snapOptions.miniCartErrorWidgetPage !== 'undefined')
            removingMiniCartErrorWidget();
    }

    if (priceTag) {
        const minOrderVal = parseNum(snapOptions.plans[0].minAmount);
        let parentDiv = '';
        parentDiv = getByIdOrClassName(snapOptions.miniCartDivId);
        if (minOrderVal > snapOrderValue) {
            let errorWidget = snapOptions.miniCartErrorWidgetPage;
            getByID('dp').textContent = downPayment;
            parentDiv.innerHTML = errorWidget;
            return;
        }

    if(snapOptions.discountPercent && snapOptions.discountPercent!== 'undefined' && snapOptions.showDiscountPercentAll && snapOptions.showDiscountPercentAll!== 'undefined' ){
         snapOrderValue=Math.round(snapOrderValue-(snapOrderValue*snapOptions.discountPercent));
    }

        for (let planIndex = 0; planIndex < snapOptions.plans.length; planIndex++) {
            const plan = snapOptions.plans[planIndex];
            if (snapOrderValue >= plan.minAmount && snapOrderValue <= plan.maxAmount) {
                downPayment = Math.round((snapOrderValue * plan.dpPercent) / 100);
                emi = Math.round(parseFloat(snapOrderValue * plan.emiPercent) / 100);
            }
        }

        if (!downPayment) {
            downPayment = snapOrderValue / 10;
        }

        if (!emi) {
            emi = parseFloat(snapOrderValue * 10) / 100.0;
        }
        let widgetPage = snapOptions.widgetPriceBreakdown && snapOptions.widgetPriceBreakdown !== "undefined" ? getMiniWidgetHtml(snapOrderValue) : snapOptions.miniCartWidgetPage;
        if (snapOptions.miniCartPayLaterAmount && snapOptions.miniCartPayLaterAmount !== 'undefined') {
            widgetPage = getPayLaterWidget(
                snapOptions.miniCartPayLaterAmount,
                snapOptions.miniCartWidgetPage,
                'miniCart'
            );
        }
        let showWidgetExpress = false;
        if (snapOptions.miniCartWidgetExpressUrl && snapOptions.miniCartWidgetExpressUrl !== 'undefined') {
            if (checkWhichWidgetToLoadMiniCart(snapOptions.miniCartWidgetExpressUrl)) {
                widgetPage = snapOptions.checkForMiniCartWidget;
                showWidgetExpress = true;
            } else {
                showWidgetExpress = false;
            }
        }
        if (snapOptions.isMiniCartEventAdd && snapOptions.isMiniCartEventAdd !== 'undefined') {
            let widget = document.querySelector('.snap_emi_txt_minicart.snap_emi_txt.snap_emi_txt_wrapper');
            if (widget) {
                widget.parentNode.removeChild(widget);
            }
        }
        if (!getByIdOrClassName('.snap_emi_txt_minicart')) {
            if (snapOptions.miniCartPosition === 'before') parentDiv.insertAdjacentHTML('beforebegin', widgetPage);
            else parentDiv.insertAdjacentHTML('afterend', widgetPage);
        }
        if(snapOptions.changeWidget && snapOptions.changeWidget !== 'undefined' && snapOptions.changeWidget !== null){
            eval(snapOptions.changeWidget);
        }
        if (document.querySelector('.snap_emi_txt_minicart'))
            addingCssStyle(snapOptions.miniCartStyle, '.snap_emi_txt_minicart');
          let elementmini = getByIdOrClassName('#sm-widget-btn.snap_emi_txt_minicart ');
           elementmini.setAttribute("data-widget_mini",snapOrderValue);
        var dp = document.querySelectorAll('.dp-class_minicart');
        if (dp.length > 0) {
            dp.forEach((element) => {
                 if((snapOptions.showPaywidgetEmi== 'true') || (snapOptions.miniCartshowPaywidgetEmii== 'true')){
                 element.textContent = emi;
                }else{
                 element.textContent = downPayment;
                }
               
            });
        }
        if (snapOptions.miniCartCashbackText && snapOptions.miniCartCashbackText !== 'undefined') {
            updateCashbackWidgetMiniCart(snapOptions.miniCartCashbackText);
        }
        if (snapOptions.merchantLogo && snapOptions.merchantLogo !== 'undefined') {
            whiteLabelChange();
        }
        if (snapOptions.addingDynamicPlan && snapOptions.addingDynamicPlan !== 'undefined') {
            addDynamicPlanMiniCart();
        }
        if (snapOptions.isAbTest && snapOptions.isAbTest !== 'undefined') {
            addDynamicData();
        }
        if (snapOptions.isCashbackCoupon && snapOptions.isCashbackCoupon !== 'undefined') {
            if (!window.location.href.includes(snapOptions.cartUrl)) {
                if (!getDynamicJsCookie('snapmintDynamicJscookie')) {
                    addClassForAutoMaticCoupon(snapOrderValue, true);
                }
            }
        }
        if (snapOptions.isMiniCartEventAdd && snapOptions.isMiniCartEventAdd !== 'undefined') {
            MiniCartOpen();
        }
        if (snapOptions.miniCartWidgetExpressUrl && snapOptions.miniCartWidgetExpressUrl !== 'undefined') {
            if (showWidgetExpress === false) {
                let element = document.querySelector('#sm-widget-btn.snap_emi_txt_minicart');
                if (element) {
                    element.style.border = 'none';
                    element.classList.add('widget_express_on_url_minicart');
                }
            }
        }
    }
}
function addingMiniCartErrorWidget(snapOrderValue) {
    const minOrderVal = parseNum(snapOptions.plans[0].minAmount);
    if (minOrderVal > snapOrderValue) {
        let parentDiv = getByIdOrClassName(snapOptions.miniCartDivId);
        let widgetPage = snapOptions.miniCartErrorWidgetPage;
        if (!getByIdOrClassName('.snap_emi_txt_minicart')) {
            if (snapOptions.miniCartPosition === 'before') parentDiv.insertAdjacentHTML('beforebegin', widgetPage);
            else parentDiv.insertAdjacentHTML('afterend', widgetPage);
        }
        if (document.querySelector('.snap_emi_txt_minicart'))
            addingCssStyle(snapOptions.miniCartStyle, '.snap_emi_txt_minicart');
        let dp = document.querySelectorAll('.dp-class_minicart');
        if (dp.length > 0) {
            dp.forEach((element) => {
                element.textContent = minOrderVal;
            });
        }
    }
}

function removingMiniCartErrorWidget() {
    if (getByIdOrClassName('.error_widget_minicart')) {
        let element = getByIdOrClassName('.error_widget_minicart');
        element.parentNode.removeChild(element);
    }
}
